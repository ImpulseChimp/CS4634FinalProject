<div id="admin-login-container">

  <h1>Admin Login</h1>

  <div id="admin-login-input-form-wrapper">

    <div class="input_wrapper">
      <input id="admin-email-address" class="admin-login-input" name="admin-username" placeholder="username"/>
    </div>

    <div class="input_wrapper">
      <input id="admin-password" class="admin-login-input" name="admin-password" placeholder="password" type="password"/>
    </div>

    <div class="input_wrapper">
      <button id="admin-login-button">LogIn</button>
    </div>

  </div>

</div>